{
  "name": "tree-care-mobile",
  "version": "0.1.0",
  "description": "Tree Care Mobile App - React Native",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo build",
    "build:ios": "expo build:ios",
    "build:android": "expo build:android",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "detox test",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "expo": "~49.0.0",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "@expo/vector-icons": "^13.0.0",
    "expo-camera": "~13.4.0",
    "expo-image-picker": "~14.3.0",
    "expo-location": "~16.1.0",
    "expo-notifications": "~0.20.0",
    "expo-secure-store": "~12.3.0",
    "expo-file-system": "~15.4.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/stack": "^6.3.0",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "react-native-screens": "~3.22.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-svg": "13.9.0",
    "@reduxjs/toolkit": "^1.9.0",
    "react-redux": "^8.1.0",
    "redux-persist": "^6.0.0",
    "aws-amplify": "^6.0.0",
    "@aws-amplify/react-native": "^1.0.0",
    "@stripe/stripe-react-native": "^0.27.0",
    "react-native-google-mobile-ads": "^12.0.0",
    "@react-native-community/image-editor": "^2.3.0",
    "react-native-image-resizer": "^1.4.5",
    "axios": "^1.4.0",
    "react-hook-form": "^7.45.0",
    "react-native-paper": "^5.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "typescript": "^5.1.3",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "@testing-library/react-native": "^12.1.0",
    "@testing-library/jest-native": "^5.4.0",
    "detox": "^20.10.0",
    "metro-config": "~0.77.0"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": ["@testing-library/jest-native/extend-expect"],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx,ts,tsx}",
      "!src/**/*.stories.{js,jsx,ts,tsx}"
    ]
  }
}
